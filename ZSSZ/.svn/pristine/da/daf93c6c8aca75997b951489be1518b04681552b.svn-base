<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="4dp">

        <TextView
            android:id="@+id/tv_detail_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerInParent="true"
            android:layout_marginBottom="6dp"
            android:text="病害明细"
            android:textColor="@color/cc_red"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_detail_reporter_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_detail_name"
            android:layout_marginTop="6dp"
            android:text="上报人："
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_reporter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_detail_name"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_detail_reporter_title"
            android:text=""
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_name_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_reporter"
            android:layout_marginTop="6dp"
            android:text="@string/report_dealgrade"
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_diseasename"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_reporter"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@+id/mysend_tv_detail_name_title"
            android:text="一类病害"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_grade_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_diseasename"
            android:layout_marginTop="6dp"
            android:text="@string/report_dealcategory"
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_grade"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_name_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@+id/mysend_tv_detail_grade_title"
            android:text="违章施工"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_dealtype_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_grade"
            android:layout_marginTop="6dp"
            android:text="@string/report_facategory"
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_dealtype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_grade"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@+id/mysend_tv_detail_dealtype_title"
            android:text="违章施工"
            android:textSize="18sp" />


        <TextView
            android:id="@+id/mysend_tv_detail_fatype_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysend_tv_detail_dealtype"
            android:layout_marginTop="6dp"
            android:text="@string/report_faname"
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_fatype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysend_tv_detail_dealtype"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/mysend_tv_detail_fatype_title"
            android:text="步道"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_pbtype_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysend_tv_detail_fatype"
            android:layout_marginTop="6dp"
            android:text="@string/report_pbtype"
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_pbtype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysend_tv_detail_fatype"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@+id/mysend_tv_detail_pbtype_title"
            android:text="塌陷"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_reporteplace_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysend_tv_detail_pbtype"
            android:layout_marginTop="6dp"
            android:text="@string/report_roadname"
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_reporteplace"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysend_tv_detail_pbtype"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/mysend_tv_detail_reporteplace_title"
            android:text="西环路"
            android:maxLines="2"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_faname_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysend_tv_detail_reporteplace"
            android:layout_marginTop="6dp"
            android:text="@string/report_facility"
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_faname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysend_tv_detail_reporteplace"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@+id/mysend_tv_detail_faname_title"
            android:text="彩砖"
            android:textSize="18sp" />


        <TextView
            android:id="@+id/mysend_tv_detail_reportetime_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_faname"
            android:layout_marginTop="6dp"
            android:text="上报时间："
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_reportetime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_faname"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/mysend_tv_detail_reportetime_title"
            android:text="2017年3月2日16:45:06"
            android:textSize="18sp" />



        <TextView
            android:id="@+id/mysend_tv_detail_sender_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_reportetime"
            android:layout_marginTop="6dp"
            android:text="要求完成人："
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <LinearLayout
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@+id/mysend_tv_detail_sender_title"
            android:layout_below="@+id/mysend_tv_detail_reportetime"
            android:id="@+id/ll_detail_sender"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/mysend_tv_detail_sender"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="向阳天 "
                android:textSize="18sp" />

            <TextView
                android:id="@+id/mysend_tv_detail_replace_person"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="修改下派人员"
                android:textSize="16sp"
                android:background="@color/cc_supervise"
                android:textColor="@color/white"
                android:padding="4dp"
                android:layout_height="wrap_content" />
        </LinearLayout>



        <TextView
            android:id="@+id/mysend_tv_detail_requesttime_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_detail_sender"
            android:layout_marginTop="6dp"
            android:text="要求完成时间："
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_detail_requesttime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_detail_sender"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/mysend_tv_detail_requesttime_title"
            android:text="2017年3月2日16:45:06"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_send_detail_loca_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_requesttime"
            android:layout_marginTop="6dp"
            android:text="病害位置："
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/mysend_tv_send_detail_loca"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_requesttime"
            android:layout_marginTop="6dp"
            android:gravity="center"
            android:textColor="@color/white"
            android:background="@color/cc_supervise"
            android:layout_toRightOf="@+id/mysend_tv_send_detail_loca_title"
            android:text="点击查看"
            android:textSize="18sp" />



        <TextView
            android:id="@+id/mysend_tv_detail_loca_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:layout_below="@+id/mysend_tv_send_detail_loca"
            android:text="位置描述："
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />


        <FrameLayout
            android:layout_marginTop="6dp"
            android:id="@+id/mysend_tv_detail_problem_loca1"
            android:layout_below="@+id/mysend_tv_send_detail_loca"
            android:layout_toRightOf="@+id/mysend_tv_detail_loca_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:visibility="gone"
                android:background="@drawable/selector_tv_audio"
                android:id="@+id/mysend_tv_detail_problem_audio"
                android:layout_width="120dp"
                android:layout_height="28dp"
                android:gravity="center"
                android:drawableRight="@mipmap/play"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/mysend_tv_detail_problem_loca"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="20sp" />


        </FrameLayout>



        <TextView
            android:layout_marginTop="6dp"
            android:layout_below="@+id/mysend_tv_detail_problem_loca1"
            android:id="@+id/mysend_tv_detail_diseasedestitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="@color/cc_text_blank"
            android:text="@string/report_problemdes"/>

        <TextView
            android:layout_marginTop="6dp"
            android:id="@+id/mysend_tv_detail_diseasedes"
            android:textSize="18sp"
            android:text="miaoshu"
            android:layout_below="@+id/mysend_tv_detail_problem_loca1"
            android:layout_toRightOf="@+id/mysend_tv_detail_diseasedestitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


        <LinearLayout
            android:layout_marginTop="6dp"
            android:id="@+id/ll_state"
            android:layout_below="@+id/mysend_tv_detail_diseasedes"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:text="病害状态："
                android:textColor="@color/cc_text_blank"
                android:textSize="18sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:padding="3dp"
                android:background="@drawable/btn_normal"
                android:id="@+id/tv_mysend_state"
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:textSize="18sp" />
            <TextView
                android:layout_marginLeft="6dp"
                android:id="@+id/tv_state"
                android:text="流程明细"
                android:textColor="@color/cc_red"
                android:padding="3dp"
                android:textSize="18sp"
                android:background="@drawable/btn_normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>


        <TextView
            android:id="@+id/mysend_tv_detail_reporteiamg_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_state"
            android:layout_marginTop="6dp"
            android:text="上报图片："
            android:textColor="@color/cc_text_blank"
            android:textSize="18sp" />


        <LinearLayout
            android:id="@+id/ll_iv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mysend_tv_detail_reporteiamg_title"
            android:padding="6dp"
            android:background="@drawable/shape_uncheck_information"
            android:orientation="horizontal">

            <ImageView
                android:scaleType="fitXY"
                android:id="@+id/iv_detail_photo1"
                android:layout_width="100dp"
                android:layout_height="120dp" />

            <ImageView
                android:scaleType="fitXY"
                android:id="@+id/iv_detail_photo2"
                android:layout_width="100dp"
                android:layout_height="120dp"
                android:layout_marginLeft="8dp" />

            <ImageView
                android:scaleType="fitXY"
                android:id="@+id/iv_detail_photo3"
                android:layout_width="100dp"
                android:layout_height="120dp"
                android:layout_marginLeft="8dp" />

        </LinearLayout>


    </RelativeLayout>


</ScrollView>