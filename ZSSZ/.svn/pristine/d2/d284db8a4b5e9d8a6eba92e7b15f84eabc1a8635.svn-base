<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="0px"
            android:focusable="true"
            android:focusableInTouchMode="true"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:layout_weight="1"
                    android:drawableLeft="@mipmap/unchcek_start"
                    android:drawablePadding="8dp"
                    android:text="@string/uncheck_redeal"
                    android:textSize="18sp" />

                <Button
                    android:id="@+id/bt_uncheck_predeal"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_margin="6dp"
                    android:background="@drawable/slecetor_btn_uncheck"
                    android:text="@string/reprote"
                    android:textColor="@color/white"
                    android:textSize="18sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="6dp"
                android:layout_marginTop="6dp"
                android:background="@color/graybackground" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="10dp">

                <ImageView
                    android:id="@+id/iv_predeal_icon1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@mipmap/iv_add"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/iv_predeal_icon2"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="10dp"
                    android:background="@mipmap/iv_add"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/iv_predeal_icon3"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="10dp"
                    android:background="@mipmap/iv_add"
                    android:scaleType="center" />

            </LinearLayout>


        </LinearLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@color/grayground" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:layout_weight="1"
                    android:drawableLeft="@mipmap/unchcek_start"
                    android:drawablePadding="8dp"
                    android:text="@string/uncheck_deal"
                    android:textSize="18sp" />

                <Button
                    android:id="@+id/bt_uncheck_dealing"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_margin="6dp"
                    android:background="@drawable/slecetor_btn_uncheck"
                    android:text="@string/reprote"
                    android:textColor="@color/white"
                    android:textSize="18sp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="6dp"
                android:layout_marginTop="6dp"
                android:background="@color/graybackground" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="10dp">

                <ImageView
                    android:id="@+id/iv_dealing_icon1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@mipmap/iv_add"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/iv_dealing_icon2"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="10dp"
                    android:background="@mipmap/iv_add"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/iv_dealing_icon3"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="10dp"
                    android:background="@mipmap/iv_add"
                    android:scaleType="fitXY" />

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@color/grayground" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:layout_weight="1"
                    android:drawableLeft="@mipmap/unchcek_start"
                    android:drawablePadding="8dp"
                    android:text="@string/uncheck_dealed"
                    android:textSize="18sp" />

                <Button
                    android:visibility="invisible"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_margin="6dp"
                    android:background="@drawable/slecetor_btn_uncheck"
                    android:text="@string/reprote"
                    android:textColor="@color/white"
                    android:textSize="18sp" />

            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="6dp"
                android:layout_marginTop="6dp"
                android:background="@color/graybackground" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/iv_dealed_icon1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"

                    android:background="@mipmap/iv_add"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/iv_dealed_icon2"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="10dp"
                    android:background="@mipmap/iv_add"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/iv_dealed_icon3"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="10dp"
                    android:background="@mipmap/iv_add"
                    android:scaleType="fitXY" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/uncheck_ll_count"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="6dp"
            android:visibility="visible">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@mipmap/unchcek_start"
                android:drawablePadding="6dp"
                android:text="维修数量："
                android:textSize="18sp" />

            <EditText
                android:id="@+id/uncheck_et_num"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center"
                android:inputType="number" />


            <TextView
                android:id="@+id/uncheck_tv_unit"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:background="@drawable/report_spinner"
                android:gravity="center_vertical"
                android:padding="6dp"
                android:textColorHint="@color/cc_supervise"
                android:hint="请选择单位"
                android:textSize="16sp" />

        </LinearLayout>



        <EditText
            android:id="@+id/et_repair_statu"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_margin="6dp"
            android:background="@drawable/shape_camrea_ok"
            android:gravity="top"
            android:hint="@string/check_detail_repair"
            android:maxLines="3"
            android:padding="5dp"
            android:textSize="18sp" />

        <Button
            android:id="@+id/bt_uncheck_dealed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:background="@drawable/slecetor_btn_post"
            android:text="@string/post"
            android:textColor="@color/white"
            android:textSize="18sp" />




    </LinearLayout>

    <LinearLayout
        android:visibility="gone"
        android:id="@+id/uncheck_progressbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/shape_modification_pb"
        android:gravity="center"
        >

        <ProgressBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:indeterminateDrawable="@drawable/progress_loading" />
    </LinearLayout>

    </FrameLayout>
</ScrollView>
