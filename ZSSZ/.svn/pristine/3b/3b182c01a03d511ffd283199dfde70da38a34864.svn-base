<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="3dp">

        <TextView
            android:id="@+id/tv_mycheck_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerInParent="true"
            android:layout_marginBottom="6dp"
            android:text="养护作业"
            android:textColor="@color/cc_red"
            android:textSize="26sp" />

        <TextView
            android:id="@+id/tv_mycheck_reporter_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_name"
            android:layout_marginTop="6dp"
            android:text="上报人员："
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_reporter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_name"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_reporter_title"
            android:text="张谋面"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_name_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mycheck_reporter_title"
            android:layout_marginTop="6dp"
            android:text="@string/report_dealgrade"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_diseasename"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mycheck_reporter_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_name_title"
            android:text="一类病害"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_grade_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_name_title"
            android:layout_marginTop="6dp"
            android:text="@string/report_dealcategory"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_grade"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_name_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_grade_title"
            android:text="违章施工"
            android:textSize="20sp" />



        <TextView
            android:id="@+id/tv_mycheck_dealtype_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_grade_title"
            android:layout_marginTop="6dp"
            android:text="@string/report_facategory"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_dealtype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_grade_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_dealtype_title"
            android:text="违章施工"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_fatype_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_dealtype_title"
            android:layout_marginTop="6dp"
            android:text="@string/report_faname"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_fatype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_dealtype_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_fatype_title"
            android:text="步道"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_pbtype_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_fatype_title"
            android:layout_marginTop="6dp"
            android:text="@string/report_pbtype"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_pbtype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_fatype_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_grade_title"
            android:text="塌陷"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_reporteplace_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_pbtype_title"
            android:layout_marginTop="6dp"
            android:text="上报地点："
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_reporteplace"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_pbtype_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_grade_title"
            android:text="西环路"
            android:maxLines="2"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_check_detail_faname_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mycheck_reporteplace"
            android:layout_marginTop="6dp"
            android:text="@string/report_facility"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_check_detail_faname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mycheck_reporteplace"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@+id/tv_check_detail_faname_title"
            android:text="彩砖"
            android:textSize="20sp" />




        <TextView
            android:id="@+id/tv_mycheck_reviewer_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_check_detail_faname"
            android:layout_marginTop="6dp"
            android:text="@string/check_detail_reviewer"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_reviewer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_check_detail_faname"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_reviewer_title"
            android:text="张三三"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_dealer_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_reviewer_title"
            android:layout_marginTop="6dp"
            android:text="@string/check_detail_dealer"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_dealer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_reviewer_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_dealer_title"
            android:text="王美丽"
            android:textSize="20sp" />




        <TextView
            android:id="@+id/tv_check_loca_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:layout_below="@+id/tv_mycheck_dealer"
            android:text="@string/problemlocation"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />


        <FrameLayout
            android:layout_marginTop="6dp"
            android:id="@+id/tv_check_problem_loca1"
            android:layout_below="@+id/tv_mycheck_dealer"
            android:layout_toRightOf="@+id/tv_check_loca_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:visibility="gone"
                android:background="@drawable/selector_tv_audio"
                android:id="@+id/tv_check_problem_audio"
                android:layout_width="120dp"
                android:layout_height="28dp"
                android:gravity="center"
                android:drawableRight="@mipmap/play"
                android:textSize="20sp" />


            <TextView
                android:id="@+id/tv_check_problem_loca"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:text="位置描述"
                android:textSize="20sp" />
        </FrameLayout>


        <TextView
            android:layout_marginTop="6dp"
            android:layout_below="@+id/tv_check_problem_loca1"
            android:id="@+id/tv_detail_diseasedestitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:textColor="@color/cc_text_blank"
            android:text="@string/report_problemdes"/>

        <TextView
            android:layout_marginTop="6dp"
            android:id="@+id/tv_mydetail_diseasedes"
            android:textSize="20sp"
            android:text="miaoshu"
            android:layout_below="@+id/tv_check_problem_loca1"
            android:layout_toRightOf="@+id/tv_detail_diseasedestitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


        <TextView
            android:id="@+id/tv_mydetail_loca_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mydetail_diseasedes"
            android:layout_marginTop="6dp"
            android:text="病害位置："
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mydetail_loca"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mydetail_diseasedes"
            android:layout_marginTop="6dp"
            android:gravity="center"
            android:background="@color/cc_supervise"
            android:textColor="@color/white"
            android:layout_toRightOf="@+id/tv_mydetail_loca_title"
            android:text="点击查看"
            android:textSize="20sp" />




        <TextView
            android:id="@+id/tv_mycheck_reportetime_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mydetail_loca"
            android:layout_marginTop="6dp"
            android:text="@string/check_detail_reporttime"
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/tv_mycheck_reportetime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mydetail_loca"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_reportetime_title"
            android:text="2017年3月2日16:45:06"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_reviewtime_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_reportetime_title"
            android:layout_marginTop="6dp"
            android:text="审核时间："
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_reviewtime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_reportetime_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_reviewtime_title"
            android:text="2017年3月2日16:47:06"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_requesttime_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_mycheck_road_title"
            android:layout_marginTop="6dp"
            android:text="要求完成时间："
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_requesttime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_road_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_requesttime_title"
            android:text="2017年3月2日19:47:06"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_resulttime_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_requesttime"
            android:layout_marginTop="6dp"
            android:text="实际完成时间："
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_resulttime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_requesttime"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_resulttime_title"
            android:text="2017年3月2日16:47:06"
            android:textSize="20sp" />





        <TextView
            android:id="@+id/tv_mycheck_decs_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_reviewtime_title"
            android:layout_marginTop="6dp"
            android:text="维修备注："
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_decs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_reviewtime_title"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_decs_title"
            android:maxLines="2"
            android:text=" "
            android:textSize="20sp" />

        <TextView
            android:visibility="gone"
            android:id="@+id/tv_mycheck_road_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_decs"
            android:layout_marginTop="6dp"
            android:text="详细位置："
            android:textColor="@color/cc_text_blank"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_mycheck_road"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_mycheck_decs"
            android:layout_marginTop="6dp"
            android:layout_toRightOf="@id/tv_mycheck_road_title"
            android:background="@color/cc_green"
            android:gravity="center"
            android:padding="3dp"
            android:visibility="gone"
            android:text="查看详细位置"
            android:textColor="@color/white"
            android:textSize="16sp" />

        <LinearLayout
            android:id="@+id/ll_mycheck_detail_title"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_below="@id/tv_mycheck_resulttime"
            android:layout_marginTop="3dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="处置前"
                android:textSize="18sp" />

            <TextView
                android:layout_width="0.3dp"
                android:layout_height="match_parent"
                android:background="@color/graybackground" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="处置后"
                android:textSize="18sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@+id/ll_mycheck_detail_title"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:background="@color/graybackground" />
        <!--第一段-->
        <LinearLayout
            android:id="@+id/ll_mycheck_detail_photo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/ll_mycheck_detail_title"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_mycheck_detail_report"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginRight="2dp"
                android:layout_marginTop="3dp"
                android:layout_weight="1"
                android:scaleType="centerCrop" />

            <TextView
                android:layout_width="0.3dp"
                android:layout_height="match_parent"
                android:background="@color/graybackground" />

            <ImageView
                android:id="@+id/iv_mycheck_detail_dealed"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="3dp"
                android:layout_weight="1"
                android:scaleType="centerCrop" />

        </LinearLayout>

    </RelativeLayout>
</ScrollView>